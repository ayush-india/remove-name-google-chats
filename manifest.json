{
  "manifest_version": 3,
  "name": "Username Remover",
  "version": "1.0",
  "description": "Removes all instances of 'nikkuu_u' from webpages.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Remove Username"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

