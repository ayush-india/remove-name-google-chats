{
  "manifest_version": 3,
  "name": "Remove Username",
  "version": "1.0",
  "description": "Removes all instances of 'nikkuu_u' (case-insensitive) from webpages.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Remove Username"
  }
}

